def check(s):
    se = set()
    for i in range(1,len(s)):
        se.add(s[i])
        if len(se) > 2:
            return False
        if s[i] == s[i - 1]:
            return False
    return True
for t in range(int(input())):
    s = input()
    if check(s):
        print("YES")
    else:
        print("NO")
