def inc(s):
    for i in range(1,len(s)):
        if s[i] <= s[i - 1]:
            return False
    return True
def dec(s):
    for i in range(1,len(s)):
        if s[i] >= s[i - 1]:
            return False
    return True
for t in range(int(input())):
    s = input()
    ok = False
    if len(s) >= 3:
        for i in range(1,len(s) - 1):
            if inc(s[:i + 1]) and dec(s[i:]):
                ok = True
                break
    if ok:
        print("YES")
    else:
        print("NO")
