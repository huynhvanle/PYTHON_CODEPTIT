from math import *
def check(s):
    sum = int(s[0])
    for i in range(1,len(s)):
        if abs(int(s[i]) - int(s[i - 1])) != 2:
            return False
        sum += int(s[i])
    if sum % 10 != 0:
        return False
    return True
for t in range(int(input())):
    s = input();
    if(check(s)):
        print("YES")
    else:
        print("NO")

