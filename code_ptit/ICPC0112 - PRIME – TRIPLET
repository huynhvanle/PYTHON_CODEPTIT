from math import *
from collections import deque
a = [1] * 1000006
def sieve():
    for i in range(2, int(sqrt(1000006))):
        if a[i]:
            for j in range(i * i, 1000006, i):
                a[j] = 0
sieve()
for t in range(int(input())):
    n = int(input())
    cnt = 0
    for i in range(3,n - 6):
        if a[i] and a[i + 2] and a[i + 6]:
            cnt += 1
        elif a[i] and a[i + 4] and a[i + 6]:
            cnt += 1
    print(cnt)
