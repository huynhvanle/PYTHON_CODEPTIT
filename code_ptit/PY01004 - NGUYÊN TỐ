from math import *
def isprime(n):
    if n <= 1:
        return 0
    for i in range(2, floor(sqrt(n) + 1)):
        if n % i == 0:
            return 0
    return 1
def check(n):
    cnt = 0
    for i in range(1,n):
        if gcd(n,i) == 1:
            cnt += 1
    if isprime(cnt): return 1
    return 0
t = int(input())
while t > 0:
    t -= 1
    n = int(input())
    if check(n) > 0:
        print("YES")
    else:
        print("NO")
