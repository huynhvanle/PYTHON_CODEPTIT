from math import *
def prime(n):
    s = "1"
    for i in range(2, int(n ** 0.5)):
        cnt = 0;
        while n % i == 0:
            n /= i
            cnt += 1
        if cnt > 0:
            s += ' * ' + str(i) + '^' + str(cnt)
    if n > 1:
        s += ' * ' + str(int(n)) + '^1';
    return s;
for t in range(int(input())):
    s = int(input());
    print(prime(s))

